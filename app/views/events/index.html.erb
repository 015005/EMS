<h1>Listing events</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Year1</th>
      <th>Month1</th>
      <th>Day1</th>
      <th>Hour1</th>
      <th>Minute1</th>
      <th>Year2</th>
      <th>Month2</th>
      <th>Day2</th>
      <th>Hour2</th>
      <th>Minute2</th>
      <th>Free</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.year1 %></td>
        <td><%= event.month1 %></td>
        <td><%= event.day1 %></td>
        <td><%= event.hour1 %></td>
        <td><%= event.minute1 %></td>
        <td><%= event.year2 %></td>
        <td><%= event.month2 %></td>
        <td><%= event.day2 %></td>
        <td><%= event.hour2 %></td>
        <td><%= event.minute2 %></td>
        <td><%= event.free %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
